page {
  background-color: #eeeeee;
}
.description{
  box-sizing: border-box;
}
.context{
  padding: 30rpx;
  background-color: #ffffff;
  width: auto;
  height: 210rpx;
}

textarea::-moz-placeholder{
  font-size: 16px;
  color: #aaaaaa;
}

.up_title{
  font-size: 12px;
  color: #aaaaaa;
}

.limitNum{
  position:absolute;
  right: 10px;
  bottom:0px;
  color: #888;
}
.ui_uploader_cell{
  height: auto;
  background-color: #ffffff;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;

}
.ui_uploader_cell .line{
  margin: 30rpx auto;
  width: 98%;
  border-bottom: 1px solid #D7D7D7;
}
.add-img{
  width: 165rpx;
  height: 165rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: #F7F7F7;
  font-size: 10px;
  color: #797979;
}
.add-img .camera{
  width: 55rpx;
  height: 50rpx;
}
.sub-btn{
  margin: 120rpx auto;
  margin-bottom: 60rpx;
  width: 65%;
  padding: .3em .8em;
  border-radius: 18px;
  font-size: 16px;
  line-height: 1.5;
}
.com-opt{
  margin: 0 auto;
  width: 100%;
  height: 400rpx;
  background-color: #ffffff;
}
.ui_uploader_cell {
  width: 100%;
  padding: 40rpx;
  box-sizing: border-box;
}
.ui_uploader_item{
  position: relative;
  margin-right: 30rpx;
  margin-bottom: 30rpx;
  width: 165rpx;
  height: 165rpx;
}
.ui_uploader {
  float: left;
  position: relative;
  margin-right: 25rpx;
  margin-bottom: 25rpx;
  width: 165rpx;
  height: 165rpx;
  border: 2rpx solid #d9d9d9;
  box-sizing: border-box;
}

.ui_uploader:before {
  content: " ";
  position: absolute;
  width: 4rpx;
  height: 79rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}

.ui_uploader:after {
  content: " ";
  position: absolute;
  height: 4rpx;
  width: 79rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}
.ui_uploader_item_icon{
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  background: #fff;
  border-radius: 50%;
}
.ui_uploader_item image {
  width: 100%;
  height: 100%;
}
